        {
          "_name": "apeople2",
          "_sourceTheme": "factm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"people2 factm5\" group=\"People\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"mobirise-icons2\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <select title=\"Align\" name=\"contentAlign\">\n            <option value=\"justify-content-start\">Left</option>\n            <option value=\"justify-content-center\" selected>Center</option>\n            <option value=\"justify-content-end\">Right</option>\n        </select>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <header>Card</header>\n        <input type=\"range\" inline title=\"Count\" name=\"cardCount\" min=\"1\" max=\"6\" step=\"1\" value=\"3\">\n        <input type=\"color\" title=\"Bg Color\" name=\"bgColor\" value=\"#ffffff\">\n        <input type=\"checkbox\" title=\"Icons\" name=\"showCardIcon\" checked>\n        <input type=\"color\" title=\"Icon Color\" name=\"iconColor\" value=\"#efbf04\" selected condition=\"showCardIcon\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\" checked>\n        <input type=\"checkbox\" title=\"Image\" name=\"showCardImg\" checked>\n        <input type=\"checkbox\" title=\"Name\" name=\"showCardName\" checked>\n        <input type=\"checkbox\" title=\"Profession\" name=\"showCardProf\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/factm5/components/_images/background3.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#003da5\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/factm5/components/_images/background3.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container-fluid':fullWidth, container:fullWidth==false}\">\n        <div class=\"main-container\" mbr-if=\"showTitle || showText\">\n            <h4 class=\"mbr-title mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title\">\n                <b>Our business clients feedback</b>\n            </h4>\n            <p class=\"mbr-text mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">\n                <a href=\"#\" class=\"text-black\">\n                    Work With Us\n                </a>\n            </p>\n        </div>\n        <div class=\"row card-row {{contentAlign}}\" mbr-if=\"showCardIcon || showCardText || showCardImg || showCardName || showCardProf\">\n            <div class=\"card col-12 col-md-6 col-lg-4\">\n                <div class=\"card-wrap\">\n                    <div class=\"testimonial-wrap\" mbr-if=\"showCardIcon\">\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\" style=\"color: rgb(239, 191, 4); fill: rgb(239, 191, 4);\"></span>\n                        </div>\n                    </div>\n                    <p class=\"card-text mbr-fonts-style align-center\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">\n                        \"Pcnaid has been an invaluable partner in helping us reduce our energy costs and streamline our energy procurement process. Their expertise and customer service are unmatched!\"\n                    </p>\n                    <div class=\"card-name-wrap\" mbr-if=\"showCardImg || showCardName || showCardProf\">\n                        <div class=\"card-name-image-wrap\" mbr-if=\"showCardImg\">\n                            <img src=\"@PROJECT_PATH@/assets/images/mbr-1-160x240.jpg\" alt=\"Pcnaid Exclusive Smokes Yacoub Customer Testimonial Headshot\">\n                        </div>\n                        <div class=\"card-name-text-wrap\" mbr-if=\"showCardName || showCardProf\">\n                            <p class=\"card-name mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showCardName\" data-app-selector=\".card-name\">\n                                Lisa Morgan</p>\n                            <p class=\"card-prof mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showCardProf\" data-app-selector=\".card-prof\">\n                                Operations Manager</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card col-12 col-md-6 col-lg-4\" mbr-if=\"cardCount > 1\">\n                <div class=\"card-wrap\">\n                    <div class=\"testimonial-wrap\" mbr-if=\"showCardIcon\">\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                    </div>\n                    <p class=\"card-text mbr-fonts-style align-center\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">\n                        \"The team at Pcnaid provided us with customized solutions that perfectly fit our business needs. Their commitment to helping us navigate the energy market has saved us time and money!\"\n                    </p>\n                    <div class=\"card-name-wrap\" mbr-if=\"showCardImg || showCardName || showCardProf\">\n                        <div class=\"card-name-image-wrap\" mbr-if=\"showCardImg\">\n                            <img src=\"@PROJECT_PATH@/assets/images/mbr-160x240.jpg\" alt=\"Pcnaid ATK Customs Arshad Customer Testimonial Headshot\">\n                        </div>\n                        <div class=\"card-name-text-wrap\" mbr-if=\"showCardName || showCardProf\">\n                            <p class=\"card-name mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showCardName\" data-app-selector=\".card-name\">\n                                James Rodriguez</p>\n                            <p class=\"card-prof mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showCardProf\" data-app-selector=\".card-prof\">\n                                Business Owner</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-md-6 col-lg-4\" mbr-if=\"cardCount > 2\">\n                <div class=\"card-wrap\">\n                    <div class=\"testimonial-wrap\" mbr-if=\"showCardIcon\">\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                    </div>\n                    <p class=\"card-text mbr-fonts-style align-center\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">\n                        \"Pcnaid's knowledge and professionalism stood out from the start. They helped us secure competitive energy rates and have been a trusted partner for our energy management.\"\n                    </p>\n                    <div class=\"card-name-wrap\" mbr-if=\"showCardImg || showCardName || showCardProf\">\n                        <div class=\"card-name-image-wrap\" mbr-if=\"showCardImg\">\n                            <img src=\"@PROJECT_PATH@/assets/images/people2.jpg\" alt=\"Pcnaid High Limits Smoke Shop Nawid Customer Testimonial Headshot\">\n                        </div>\n                        <div class=\"card-name-text-wrap\" mbr-if=\"showCardName || showCardProf\">\n                            <p class=\"card-name mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showCardName\" data-app-selector=\".card-name\">\n                                David Smith</p>\n                            <p class=\"card-prof mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showCardProf\" data-app-selector=\".card-prof\">\n                                CEO</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card col-12 col-md-6 col-lg-4\" mbr-if=\"cardCount > 3\">\n                <div class=\"card-wrap\">\n                    <div class=\"testimonial-wrap\" mbr-if=\"showCardIcon\">\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                        <div class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"mbr-iconfont mobi-mbri-star mobi-mbri\"></span>\n                        </div>\n                    </div>\n                    <p class=\"card-text mbr-fonts-style align-center\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">\n                        \"Sagittis purus sit amet volutpat consequat mauris nunc congue. Sed egestas egestas fringilla\n                        phasellus faucibus. \"\n                    </p>\n                    <div class=\"card-name-wrap\" mbr-if=\"showCardImg || showCardName || showCardProf\">\n                        <div class=\"card-name-image-wrap\" mbr-if=\"showCardImg\">\n                            <img src=\"@ADDONS_PATH@/factm5/components/_images/people/people2.jpg\" alt=\"Mobirise Website Builder\">\n                        </div>\n                        <div class=\"card-name-text-wrap\" mbr-if=\"showCardName || showCardProf\">\n                            <p class=\"card-name mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showCardName\" data-app-selector=\".card-name\">\n        